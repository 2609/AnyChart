[build]
command = ./build contrib && ./build compile && ./build docs

[test_group:docs]
type = html
source_type = path
source_path = ./out/docs/master/tests/*.html
screenshot_message = 'chart draw'


[test_group:qa_tests]
type = html
source_type = git
git_path = git@github.com:AnyChart/ACDVF-tests.git
git_name = ACDVF-tests
source_path = ./tests/*.html;./tests/*/*.html;./tests/*/*/*.html;
screenshot_message = 'chart draw'


[test_group:playground-samples]
type = html
source_type = git
git_path = git@github.com:AnyChart/ACDVF-tests.git
git_name = ACDVF-tests
source_path = ./playground-samples/*/*.html
screenshot_message = 'chart draw'