# this file required for testing system
# DO NOT EDIT OR REMOVE
[build]
command = ./build contrib && ./build compile@IS_EACH @IS_DEVELOP @DOCS_PARAMETER

[test_group:qa_tests]
type = html
source_type = git
git_path = git@github.com:AnyChart/ACDVF-tests.git
git_name = ACDVF-tests
tests_folder = ./tests/
source_path = ./*.html;./*/*.html;./*/*/*.html;./*/*/*/*.html;
test_tools = ./anytest.min.js
js_path = ./scripts/

[test_group:licence]
type = html
source_type = git
git_path = git@github.com:AnyChart/ACDVF-tests.git
git_name = ACDVF-tests
tests_folder = ./licence/
source_path = ./*.html;./*/*.html;
test_tools = ./anytest.min.js
js_path = ./scripts/
parameter = @IS_EACH
parameter_value = _each